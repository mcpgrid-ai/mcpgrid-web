{
  "name": "mcpbox-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3002",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "gen:ks": "graphql-codegen --config ./keystone.config.ts",
    "gen:strapi": "graphql-codegen --config ./strapi.config.ts",
    "gen:api": "yarn download:api && orval --config ./api.config.js",
    "act": "act -j deploy --secret-file .env --env-file .env --var-file .env --container-architecture linux/amd64",
    "download:api": "curl -o src/_network/api/openapi.json http://localhost:3001/docs/swagger.json -H 'Accept: application/json'"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@iconify/react": "^6.0.0",
    "@next/third-parties": "^15.5.0",
    "@octokit/rest": "^22.0.0",
    "@tanstack/react-query": "^5.83.0",
    "axios": "^1.10.0",
    "bootstrap": "^5.3.7",
    "classnames": "^2.5.1",
    "graphql": "^16.11.0",
    "lodash": "^4.17.21",
    "next": "15.3.4",
    "next-intl": "^4.3.0",
    "notistack": "^3.0.2",
    "qs": "^6.14.0",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.10",
    "react-bootstrap-typeahead": "^6.4.1",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-router": "^7.6.2",
    "react-syntax-highlighter": "^15.6.1",
    "react-use": "^17.6.0",
    "scheduler": "^0.26.0",
    "use-context-selector": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.8.0",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/client-preset": "^4.8.2",
    "@graphql-codegen/typescript-document-nodes": "^4.0.16",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@types/lodash": "^4.17.18",
    "@types/node": "^20",
    "@types/qs": "^6.14.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.5.0",
    "orval": "^7.10.0",
    "prettier": "^3.5.3",
    "sass": "^1.89.2",
    "type-fest": "^4.41.0",
    "typescript": "^5"
  }
}
